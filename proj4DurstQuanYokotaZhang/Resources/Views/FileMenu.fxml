<?xml version="1.0" encoding="UTF-8"?>

<!--imported packages-->
<?import javafx.scene.control.*?>

<Menu mnemonicParsing="false" text="File" fx:controller="proj4DurstQuanYokotaZhang.Controllers.FileMenuController" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <MenuItem mnemonicParsing="false" text="About..." fx:id="aboutButton" onAction="#handleAboutAction" />
    <SeparatorMenuItem/>
    <MenuItem mnemonicParsing="false" text="New" fx:id="newMenuButton" onAction="#handleNewAction" accelerator="Shortcut+N"/>
    <MenuItem mnemonicParsing="false" text="Open" fx:id="openMenuButton" onAction="#handleOpenAction" accelerator="Shortcut+O"/>
    <MenuItem mnemonicParsing="false" text="Save As..." fx:id="saveAsMenuButton" onAction="#handleSaveAsAction" />
    <MenuItem mnemonicParsing="false" text="Save" fx:id="saveMenuButton" onAction="#handleSaveAction" accelerator="Shortcut+S"/>
    <MenuItem mnemonicParsing="false" text="Close" fx:id="closeMenuButton" onAction="#handleCloseAction" accelerator="Shortcut+W"/>
    <SeparatorMenuItem/>
    <MenuItem mnemonicParsing="false" text="Exit" fx:id="exitMenuButton" onAction="#handleExitAction" accelerator="Shortcut+Q"/>
</Menu>